{
  "version": 1,
  "name": "example",
  "group": "default",
  "order": 0,
  "request": {
    "method": "GET",
    "path": "/todos/1"
  },
  "checks": [
    {
      "type": "statusCodeEqual",
      "data": {
        "value": 200
      }
    },
    {
      "type": "jsonBodyEqual",
      "data": {
        "value": {
          "userId": 1,
          "id": 1,
          "title": "delectus aut autem",
          "completed": false
        }
      }
    },
    {
      "type": "jsonBodyQueryExists",
      "data": {
        "query": "title"
      }
    },
    {
      "type": "jsonBodyQueryEqual",
      "data": {
        "query": "userId",
        "value": 1
      }
    },
    {
      "type": "jsonBodyQueryRegexMatch",
      "data": {
        "query": "title",
        "pattern": "[a-z]{8} [a-z]{3} [a-z]{5}"
      }
    }
  ]
}


